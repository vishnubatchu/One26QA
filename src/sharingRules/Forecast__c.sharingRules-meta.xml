            <group>Forecast_Non_WSS_Users</group>
        </sharedTo>
        <criteriaItems>
            <field>BusinessUnit__c</field>
            <operation>notEqual</operation>
            <value>WSS</value>
        </criteriaItems>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>WSSOnlySharing</fullName>
        <accessLevel>Read</accessLevel>
        <label>WSSOnlySharing</label>
        <sharedTo>
            <group>Forecast_WSS_Users</group>
        </sharedTo>
        <criteriaItems>
            <field>BusinessUnit__c</field>
            <operation>equals</operation>
            <value>WSS</value>
        </criteriaItems>
    </sharingCriteriaRules>
</SharingRules>
