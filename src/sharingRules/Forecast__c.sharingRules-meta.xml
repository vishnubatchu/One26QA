        <accessLevel>Read</accessLevel>
        <label>ForecastCommonAccess</label>
        <sharedTo>
            <group>Forecast_Common_Users</group>
        </sharedTo>
        <criteriaItems>
            <field>CreatedById</field>
            <operation>notEqual</operation>
            <value>null</value>
        </criteriaItems>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>NonWSSSharing</fullName>
        <accessLevel>Read</accessLevel>
        <label>NonWSSSharing</label>
