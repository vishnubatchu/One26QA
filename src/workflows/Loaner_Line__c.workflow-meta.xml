    <rules>
        <fullName>Loaner Item Return Reminder Workflow Rule 14Days Reminder</fullName>
        <active>true</active>
        <description>Sends Email When  Return Due Date is before 14 Days</description>
        <formula>AND (    NOT(ISBLANK( Order_Line__c )),    Balance_to_be_returned__c &gt;0,    Ship_Status__c= &apos;Shipped&apos;    )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Loaner_Return_Date_Reminder_2Week_Notfcn_Alert</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Loaner_Line__c.Effective_Return_Due_Date__c</offsetFromField>
            <timeLength>-14</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Loaner Item Return Reminder Workflow Rule 7 Days Reminder</fullName>
        <active>true</active>
        <description>Sends Email When  Return Due Date is before 7 Days Reminder</description>
        <formula>AND (    NOT(ISBLANK( Order_Line__c )),    Balance_to_be_returned__c &gt;0,    Ship_Status__c= &apos;Shipped&apos;    )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Loaner_Line_Return_Due_Date_Reminder_Email_Alert</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Loaner_Line__c.Effective_Return_Due_Date__c</offsetFromField>
            <timeLength>-7</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Update Inside Sales Finisar Email</fullName>
        <actions>
            <name>Update_Inside_Sales_Finisar_Email</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Update_Inside_Sales_Sales_Rep</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Update Inside Sales Finisar From Formula Field</description>
        <formula>OR(   NOT(ISBLANK(Loaner__r.Inside_Sales_Finisar__c )) , NOT(ISBLANK(Loaner__r.Inside_Sales_Sales_rep__c ) )  )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update Loaner Line Status To read Only</fullName>
        <actions>
            <name>Update_Record_Type_To_Read_Only</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Loaner_Line__c.Line_Status__c</field>
            <operation>equals</operation>
            <value>Approved</value>
        </criteriaItems>
        <criteriaItems>
            <field>Loaner_Line__c.Initial_Error_Message_Present__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <criteriaItems>
            <field>Loaner_Line__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>Loaner Item,Loaner Item Read Only,Loaner Item Pending Sales Record Type</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
